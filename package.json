{
  "name": "@easymfa/easymfa-cli",
  "version": "1.1.0",
  "description": "EasyMFA is a command line application that gets AWS credentials given a MFA token and serial number and saves it to a specified profile in the AWS credential file.",
  "main": "src/index.js",
  "bin": {
    "easymfa": "src/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node src/index.js",
    "prepublishOnly": "npm run build && npm run test",
    "test": "jest"
  },
  "author": "Gene Pian",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gpian/easymfa-cli.git"
  },
  "keywords": [
    "aws",
    "mfa",
    "sts",
    "aws-sts",
    "get-session-token",
    "session",
    "token",
    "cli",
    "profile",
    "2-factor",
    "multi-factor",
    "credentials"
  ],
  "dependencies": {
    "aws-sdk": "^2.394.0",
    "colors": "^1.3.3",
    "prop-ini": "0.0.2",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "jest": "^24.0.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.4"
  }
}
